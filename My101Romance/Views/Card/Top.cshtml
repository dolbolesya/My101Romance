@model IEnumerable<My101Romance.Domain.Entity.Card>

<div class="container">
    <div class="row">
        @foreach (var card in Model)
        {
            <div class="col-md-12 my-3">
                <div>
                    <p>R: @card.Rating</p>
                    <p>@card.Title</p>
                    <p>@card.Description</p>
                    <hr/>
                </div>
            </div>
        }
    </div>
</div>
