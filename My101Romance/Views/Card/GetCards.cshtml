@model IEnumerable<My101Romance.Domain.Entity.Card>
@{
    Layout = "_Layout";
}

    @foreach (var card in Model)
    {
        
        <p>@card.Id</p>
        @if (true)
        {
            
            <p>@card.Title</p>
            <p>@card.Description</p>
            
        }
        <hr/>
    }

